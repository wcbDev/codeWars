function filter_list(l) {
  let integers = []
  for(let i = 0; i < l.length; i++){
  if(Number.isInteger(l[i])){
  integers.push(l[i])
    } 
  }
  return integers
}


function filter_list(l) {
  return l.filter(e => Number.isInteger(e));
}

function filter_list(l) {
  return l.filter(function(v) {return typeof v == 'number'})
}
